{
  "coachingAI": {
    "version": "1.0.0",
    "name": "PersonalCoachAI",
    "configuration": {
      "mode": "interactive",
      "supportedDomains": [
        "professional development",
        "personal growth",
        "skill acquisition",
        "performance coaching"
      ],
      "languageSupport": [
        "en-US",
        "en-GB"
      ]
    },
    "capabilities": {
      "assessmentTypes": [
        "skill-gap",
        "personality-analysis",
        "goal-alignment"
      ],
      "coachingMethods": [
        "one-on-one",
        "group",
        "self-guided"
      ]
    },
    "personalizedSettings": {
      "userProfileRequired": true,
      "dataPrivacy": {
        "anonymization": true,
        "dataRetention": "30 days"
      }
    },
    "aiModules": {
      "conversationEngine": {
        "type": "natural-language-processing",
        "contextAwareness": true,
        "emotionalIntelligence": true
      },
      "recommendationSystem": {
        "algorithmType": "collaborative-filtering",
        "personalizedScoring": true
      }
    },
    "integrations": {
      "supportedPlatforms": [
        "web",
        "mobile",
        "desktop"
      ],
      "dataExportFormats": [
        "JSON",
        "CSV",
        "PDF"
      ]
    }
  }
}
